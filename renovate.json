{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prConcurrentLimit": 10,
  "packageRules": [
    {
      "automerge": true,
      "matchDatasources": [
        "maven"
      ],
      "versioning": "semver",
      "bumpVersion": "patch"
    }
  ],
  "labels": ["renovate"]
}